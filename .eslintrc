{
    "rules": {
        "indent": [
            2,
            4,
            {"SwitchCase": 1}
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
            1
        ]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "require": false,
        "module": false,
        "exports": true
    },
    "extends": "eslint:recommended"
}
